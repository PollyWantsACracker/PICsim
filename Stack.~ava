import java.util.*;

public class Stack {
  
  private List<Integer> stack;
  
  public Stack() {
    
    stack = new ArrayList<Integer>();
    
  }
  
  public void addAddress(int address) {
    
    stack.add(address);
    
  }
  
  public int getLastAddress() {
    
    int address = stack.get(stack.size() - 1);
    stack.remove(stack.size() - 1);
    return address;
    
  }
  
}